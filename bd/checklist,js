module.exports = (sequelize, DataTypes) => {
    const checklist = sequelize.define('UChecklist',{
        id:{
        type: DataTypes.INTEGER,
        allowNull: false,
        primaryKey: true,
        autoIncrement: true,
        },
        notaId: {
            type: DataTypes.STRING,
            allowNull: false,
        },
        descricao:{
            type:DataTypes.TEXT,
            allowNull: false,
        },
        concluida:{
            type:DataTypes.BOOELAND,
            allowNull: false,
        },
        
    },
    {
        tableName: "checklist",
        timestamps: false,
    }
    );
    return checklist
}